{
    "expo": {
        "name": "ssalad",
        "version": "1.0.0",
        "slug": "ssalad-app",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "package": "com.jaminlab.sslad",
            "versionCode": 6
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
